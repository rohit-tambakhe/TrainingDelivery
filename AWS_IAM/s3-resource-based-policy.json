{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "Statement1",
			"Effect": "Deny",
			"Principal": {
				"AWS": "arn:aws:iam::YOUR_ACCOUNT_ID:user/YOUR_ACCOUNT_USER_NAME"
			},
			"Action": "S3:*",
			"Resource": [
				"arn:aws:s3:::YOUR_S3_BUCKET_NAME",
				"arn:aws:s3:::YOUR_S3_BUCKET_NAME/*"
			]
		}
	]
}